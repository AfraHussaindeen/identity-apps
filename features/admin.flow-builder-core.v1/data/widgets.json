                            "width": 350,
                            "height": 291
                        },
                        "position": {
                            "x": 300,
                            "y": 200
                        },
                        "data": {
                            "action": {
                                "type": "EXECUTOR",
                                "executor": {
                                    "name": "GoogleOIDCAuthenticator",
                                    "meta": {
